{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/users/src",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/users",
        "main": "apps/users/src/main.ts",
        "tsConfig": "apps/users/tsconfig.app.json",
        "externalDependencies": "all"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/user/src/environments/environment.ts",
              "with": "apps/user/src/environments/environment.prod.ts"
            }
          ],
          "externalDependencies": "none"
        }
      }
    },
    "serve": {
      "executor": "@ns3/nx-serverless:sls",
      "options": {
        "command": "offline",
        "buildTarget": "users:build"
      }
    },
    "package": {
      "executor": "@ns3/nx-serverless:sls",
      "outputs": ["apps/users/.serverless", "dist/apps/users"],
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        }
      ],
      "options": {
        "command": "package",
        "buildTarget": "users:build:production"
      }
    },
    "deploy": {
      "executor": "@ns3/nx-serverless:sls",
      "outputs": ["apps/users/.serverless", "dist/apps/users"],
      "dependsOn": [
        {
          "target": "package",
          "projects": "self"
        }
      ],
      "options": {
        "command": "deploy",
        "package": ".serverless",
        "buildTarget": "users:build:production"
      }
    },
    "remove": {
      "executor": "@ns3/nx-serverless:sls",
      "options": {
        "command": "remove"
      }
    },
    "sls": {
      "executor": "@ns3/nx-serverless:sls",
      "options": {}
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/users"],
      "options": {
        "jestConfig": "apps/users/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/users/src/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
